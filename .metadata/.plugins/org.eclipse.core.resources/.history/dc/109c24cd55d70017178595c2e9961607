 import java.net.*;
 import java.io.*;
 
public class CheckSum {
	public static void main(String[] args) throws Exception {
		
		URL input = new URL("http://adventofcode.com/2017/day/2/input");
	    BufferedReader in = new BufferedReader( new InputStreamReader(input.openStream()) );

	    String inputLine;
	    StringBuilder sb = new StringBuilder(); 
	    while ((inputLine = in.readLine()) != null)
	    {
	    	sb.append(inputLine);
	    }
	    in.close();
	    String testInput = sb.toString();
		
		String[] answer = testInput.split(" +");
		int total = 0;
		int count = 0;
		int tempMin = Integer.parseInt(answer[0]);
		int tempMax = Integer.parseInt(answer[0]);
		for(int i = 0; i < answer.length; i++)
		{
			if(count < 16)
			{
				if(tempMin > Integer.parseInt(answer[i]))
				{
					tempMin = Integer.parseInt(answer[i]);
				}
				total += Integer.parseInt(answer[i]);
			}
			else
			{
				count = 0;
			}
		}
		System.out.print(total);
	}
}
