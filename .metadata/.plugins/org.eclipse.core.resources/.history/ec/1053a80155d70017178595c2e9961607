 import java.net.*;
 import java.io.*;
 
public class CheckSum {
	public static void main(String[] args) throws Exception {
		URL input = new URL("http://adventofcode.com/2017/day/2/input");
	    BufferedReader in = new BufferedReader( new InputStreamReader(input.openStream()) );

	    String inputLine;
	    StringBuilder sb = new StringBuilder();
	    String inputAppended ="";
	    while ((inputLine = in.readLine()) != null)
	    {
	    	sb.append(inputLine);
	    }
	    in.close();
		String testInput = "409	194	207	470	178	454	235	333	511	103	474	293	525	372	408	428";
		
		String[] answer = testInput.split(" +");
		int total = 0;
		int offcentre = (answer.length)/2;
		for(int i = 0; i < answer.length; i++)
		{
			if(i+offcentre >= answer.length)
			{
				int check = (i+offcentre)%answer.length;
				if(answer[i].equals(answer[check]))
				{
					total += Integer.parseInt(answer[i]);
				}
			}
			else if(answer[i].equals(answer[i+offcentre]))
			{
				total += Integer.parseInt(answer[i]);
			}
		}
		System.out.print(total);
	}
}
